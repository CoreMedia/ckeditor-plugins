{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "../dist",
    "esModuleInterop": true,
    "types": [
      "jest",
      "playwright",
      "wait-for-expect",
      "jest-playwright-preset",
      "expect-playwright",
      "node"
    ],
    "paths": {
