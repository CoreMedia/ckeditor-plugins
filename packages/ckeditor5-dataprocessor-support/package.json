{
  "name": "@coremedia/ckeditor5-dataprocessor-support",
  "version": "15.0.2-rc.4",
  "description": "Utilities for providing custom CKEditor 5 DataProcessors",
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "dataprocessor"
  ],
  "engines": {
    "node": "18",
    "pnpm": "^8.6.9"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc --project ./tsconfig.release.json",
    "clean": "pnpm clean:src && pnpm clean:dist",
    "clean:src": "rimraf --glob \"src/**/*.@(js|js.map|d.ts|d.ts.map)\"",
    "clean:dist": "rimraf ./dist",
    "jest": "jest",
    "jest:coverage": "jest --collect-coverage",
    "npm-check-updates": "npm-check-updates --upgrade"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-core": "^39.0.2",
    "@ckeditor/ckeditor5-engine": "^39.0.2",
    "@ckeditor/ckeditor5-utils": "^39.0.2",
    "@coremedia-internal/ckeditor5-jest-test-helpers": "^1.0.0",
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "jest-each": "^29.5.0",
    "jest-xml-matcher": "^1.2.0",
    "rimraf": "^5.0.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@ckeditor/ckeditor5-core": "^39.0.2",
    "@ckeditor/ckeditor5-engine": "^39.0.2"
  },
  "dependencies": {
    "@coremedia/ckeditor5-common": "15.0.2-rc.4",
    "@coremedia/ckeditor5-logging": "15.0.2-rc.4"
  }
}
