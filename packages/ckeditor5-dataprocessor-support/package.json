    "jest-xml-matcher": "^1.2.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@ckeditor/ckeditor5-core": "^37.0.1",
    "@ckeditor/ckeditor5-engine": "^37.0.1"
  },
  "dependencies": {
