{
  "name": "@coremedia/coremedia-studio-integration",
  "version": "28.2.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc --build src && babel ./src --out-dir ./dist --extensions \".ts,.js\"",
    "postbuild": "copyfiles -u 2 src/theme/* src/theme/**/* dist/theme",
    "clean": "rimraf ./dist",
    "jest": "jest --passWithNoTests",
    "publishpkg": "npm publish",
    "jest:coverage": "jest --collect-coverage --passWithNoTests",
    "lint": "eslint --fix \"src/**\"",
    "prettier": "prettier --write \"src/*.+(js|jsx|ts|tsx|json)\"",
    "npm-check-updates": "npm-check-updates --upgrade"
  },
  "exports": {
    "./*": "./dist/*.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.12.1",
    "@babel/plugin-transform-computed-properties": "^7.12.1",
    "@babel/plugin-transform-for-of": "^7.14.5",
    "@babel/plugin-transform-parameters": "^7.12.1",
    "@babel/plugin-transform-shorthand-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@coremedia-internal/jest-test-helpers": "^1.0.0",
    "@types/jest": "^26.0.20",
    "@types/jest-each": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-jest": "^27.0.6",
    "copyfiles": "^2.4.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^27.0.6",
    "jest-each": "^27.0.6",
    "jest-xml-matcher": "^1.2.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2",
    "@coremedia/studio-apps-service-agent": "^0.0.31"
  },
  "peerDependencies": {
    "@coremedia/studio-apps-service-agent": "^0.0.31"
  },
  "dependencies": {
    "rxjs": "^6.5.4"
  }
}
