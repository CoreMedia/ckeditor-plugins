    "@coremedia-internal/ckeditor5-babel-config": "^1.0.0",
    "@coremedia-internal/ckeditor5-jest-test-helpers": "^1.0.0",
    "@coremedia/service-agent": "^1.1.5",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
