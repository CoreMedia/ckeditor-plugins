{
  "name": "@coremedia/ckeditor5-font-mapper",
  "description": "Replaces characters in given font to alternative representation on paste",
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "font",
    "clipboard"
  ],
  "main": "./src/index.ts",
  "publishConfig": {
    "main": "./src/index.js",
    "types": "./src/index.d.ts"
  },
  "version": "14.1.2-rc.0",
  "engines": {
    "node": "18",
    "pnpm": "^8.1"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@ckeditor/ckeditor5-clipboard": "^37.0.1",
    "@ckeditor/ckeditor5-core": "^37.0.1",
    "@ckeditor/ckeditor5-engine": "^37.0.1",
    "@ckeditor/ckeditor5-utils": "^37.0.1",
    "@coremedia-internal/ckeditor5-jest-test-helpers": "^1.0.0",
    "@types/jest": "^29.5.0",
    "@types/webpack": "^5.28.1",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@ckeditor/ckeditor5-clipboard": "^37.0.1",
    "@ckeditor/ckeditor5-core": "^37.0.1",
    "@ckeditor/ckeditor5-engine": "^37.0.1",
    "@ckeditor/ckeditor5-utils": "^37.0.1"
  },
  "dependencies": {
    "@coremedia/ckeditor5-common": "14.1.2-rc.0",
    "@coremedia/ckeditor5-core-common": "14.1.2-rc.0",
    "@coremedia/ckeditor5-logging": "14.1.2-rc.0"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.release.json",
    "clean": "pnpm clean:src && clean:dist",
    "clean:src": "rimraf \"src/**/*.@(js|d.ts)\"",
    "clean:dist": "rimraf ./dist",
    "jest": "jest",
    "jest:coverage": "jest --collect-coverage",
    "npm-check-updates": "npm-check-updates --upgrade"
  }
}
