{
  "name": "@coremedia/ckeditor5-core-common",
  "version": "26.0.1-rc.0",
  "type": "module",
  "description": "Assistive Utilities for `@ckeditor/ckeditor5-core`.",
  "author": {
    "name": "CoreMedia GmbH",
    "email": "info@coremedia.com",
    "url": "https://coremedia.com/",
    "avatar": "data:image/svg+xml;base64,"
  },
  "keywords": [
    "coremedia",
    "ckeditor",
    "ckeditor5",
    "ckeditor5-core"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/src/index.js",
      "import": "./dist/src/index.js"
    }
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "exports": {
      ".": {
        "types": "./src/index.d.ts",
        "default": "./src/index.js"
      }
    }
  },
  "files": [
    "/src"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "ckeditor5": "46.1.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "ckeditor5": "46.1.1"
  },
  "dependencies": {
    "@coremedia/ckeditor5-common": "26.0.1-rc.0",
    "@coremedia/ckeditor5-logging": "26.0.1-rc.0"
  },
  "scripts": {
    "clean": "pnpm clean:src && pnpm clean:dist",
    "clean:src": "rimraf --glob \"src/**/*.@(js|js.map|d.ts|d.ts.map)\"",
    "clean:dist": "rimraf ./dist",
    "build": "tsc --project ./tsconfig.release.json",
    "postbuild": "node prepare-package.cjs && copyfiles ./README.md dist",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "npm-check-updates": "npm-check-updates --upgrade"
  }
}
