{
  "name": "@coremedia-internal/ckeditor5-coremedia-itest",
  "version": "1.0.0",
  "description": "Tests based on Playwright Test Framework",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "exit 0",
    "postbuild": "exit 0",
    "clean": "rimraf ./allure-report ./allure-results ./playwright-report ./results.*",
    "jest": "exit 0",
    "jest:coverage": "exit 0",
    "lint": "eslint --fix \"__itests__/**\"",
    "prettier": "prettier --write \"__itests__/*.+(js|jsx|ts|tsx|json)\"",
    "npm-check-updates": "npm-check-updates --upgrade",
    "preplaywright": "tsc --incremental -p ./__itests__",
    "playwright": "playwright test --config ./__itests__-out",
    "playwright-open": "playwright show-report",
    "allure-generate": "allure generate ./allure-results --clean",
    "allure-open": "allure open ./allure-report",
    "start": "http-server ../../app --utc --port 3000"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-editor-classic": "^32.0.0",
    "@coremedia/types-ckeditor__ckeditor5-editor-classic": "4.0.1-rc.5",
    "@playwright/test": "^1.20.0",
    "allure-commandline": "^2.17.2",
    "allure-playwright": "^2.0.0-beta.15",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "http-server": "^14.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@ckeditor/ckeditor5-editor-classic": "^32.0.0"
  }
}
