{
  "name": "@coremedia/ckeditor5-coremedia-studio-integration",
  "version": "1.0.2-rc.1",
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc --build src && babel ./src --out-dir ./dist --extensions \".ts,.js\"",
    "postbuild": "copyfiles -u 2 src/theme/* src/theme/**/* dist/theme",
    "clean": "rimraf ./dist",
    "jest": "jest --passWithNoTests",
    "jest:coverage": "jest --collect-coverage --passWithNoTests",
    "lint": "eslint --fix \"src/**\"",
    "prettier": "prettier --write \"src/*.+(js|jsx|ts|tsx|json)\"",
    "npm-check-updates": "npm-check-updates --upgrade"
  },
  "exports": {
    "./*": "./dist/*.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.16.0",
    "@babel/plugin-transform-computed-properties": "^7.16.0",
    "@babel/plugin-transform-for-of": "^7.16.0",
    "@babel/plugin-transform-parameters": "^7.16.3",
    "@babel/plugin-transform-shorthand-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@coremedia-internal/ckeditor5-jest-test-helpers": "^1.0.0",
    "@coremedia/service-agent": "^1.0.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-jest": "^27.3.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "jest-each": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "jest-xml-matcher": "^1.2.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@coremedia/service-agent": "^1.0.0"
  },
  "dependencies": {
    "@coremedia/ckeditor5-logging": "1.0.2-rc.1",
    "rxjs": "^6.5.4"
  }
}
