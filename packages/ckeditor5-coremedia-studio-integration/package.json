{
  "name": "@coremedia/ckeditor5-coremedia-studio-integration",
  "version": "8.0.1-rc.0",
  "engines": {
    "node": ">=15.0.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc --build src && babel ./src --out-dir ./dist --extensions \".ts,.js\"",
    "postbuild": "copyfiles -u 2 src/theme/* src/theme/**/* dist/theme",
    "clean": "rimraf ./dist",
    "jest": "jest --passWithNoTests",
    "jest:coverage": "jest --collect-coverage --passWithNoTests",
    "lint": "eslint --fix \"src/**\"",
    "prettier": "prettier --write \"src/*.+(js|jsx|ts|tsx|json)\"",
    "npm-check-updates": "npm-check-updates --upgrade"
  },
  "exports": {
    "./*": "./dist/*.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@coremedia-internal/ckeditor5-babel-config": "^1.0.0",
    "@coremedia-internal/ckeditor5-jest-test-helpers": "^1.0.0",
    "@coremedia/service-agent": "^1.1.0",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "jest": "^28.1.3",
    "jest-each": "^28.1.3",
    "jest-xml-matcher": "^1.2.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "@coremedia/service-agent": "^1.1.0"
  },
  "dependencies": {
    "@coremedia/ckeditor5-logging": "8.0.1-rc.0",
    "rxjs": "^7.5.5"
  }
}
