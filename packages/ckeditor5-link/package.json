{
  "name": "@coremedia/ckeditor5-link",
  "version": "28.0.2",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc --build src && babel ./src --out-dir ./dist --extensions \".ts,.js\"",
    "postbuild": "copyfiles -u 2 src/theme/* src/theme/**/* dist/theme",
    "clean": "rimraf ./dist",
    "publish": "jangaroo publish ./dist",
    "jest": "jest --passWithNoTests",
    "jest:coverage": "jest --collect-coverage --passWithNoTests",
    "lint": "eslint --fix \"src/**\"",
    "prettier": "prettier --write \"src/*.+(js|jsx|ts|tsx|json)\"",
    "npm-check-updates": "npm-check-updates --upgrade"
  },
  "exports": {
    "./": "./dist/"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.12.1",
    "@babel/plugin-transform-computed-properties": "^7.12.1",
    "@babel/plugin-transform-parameters": "^7.12.1",
    "@babel/plugin-transform-shorthand-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@coremedia-internal/jest-test-helpers": "^1.0.0",
    "@jangaroo/publish": "^1.0.0-alpha",
    "@types/jest": "^26.0.20",
    "@types/jest-each": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-each": "^26.6.2",
    "jest-xml-matcher": "^1.2.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-core": "^28.0.0",
    "@ckeditor/ckeditor5-engine": "^28.0.0",
    "@ckeditor/ckeditor5-link": "^28.0.0",
    "@ckeditor/ckeditor5-ui": "^28.0.0",
    "@ckeditor/ckeditor5-utils": "^28.0.0",
    "@coremedia/coremedia-utils": "^28.0.0"
  }
}
