{
  "name": "@coremedia/ckeditor5-logging",
  "version": "8.1.1-rc.3",
  "description": "Provides logging to use in the plugins.",
  "keywords": [
    "coremedia"
  ],
  "engines": {
    "node": ">=15.0.0",
    "pnpm": "7"
  },
  "exports": {
    "./*": "./dist/*.js"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@coremedia-internal/ckeditor5-babel-config": "^1.0.0",
    "@coremedia-internal/ckeditor5-jest-test-helpers": "^1.0.0",
    "@types/jest": "^28.1.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.16",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc --build src && babel ./src --out-dir ./dist --extensions \".ts,.js\"",
    "jest": "jest",
    "jest:coverage": "jest --collect-coverage",
    "lint": "eslint --fix \"src/**\"",
    "prettier": "prettier --write \"src/*.+(js|jsx|ts|tsx|json)\"",
    "npm-check-updates": "npm-check-updates --upgrade"
