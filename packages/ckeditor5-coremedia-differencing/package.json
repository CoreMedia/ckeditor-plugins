{
  "name": "@coremedia/ckeditor5-coremedia-differencing",
  "version": "25.0.4-rc.1",
  "type": "module",
  "description": "Server-side differencing support in CoreMedia Studio.",
  "author": {
    "name": "CoreMedia GmbH",
    "email": "info@coremedia.com",
    "url": "https://coremedia.com/",
    "avatar": "data:image/svg+xml;base64,"
  },
  "keywords": [
    "coremedia",
    "ckeditor",
    "ckeditor5",
    "differencing"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/src/index.js",
      "import": "./dist/src/index.js"
    }
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "exports": {
      ".": {
        "types": "./src/index.d.ts",
        "default": "./src/index.js"
      }
    }
  },
  "files": [
    "/src"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@coremedia-internal/ckeditor5-coremedia-example-data": "^1.0.0",
    "@coremedia-internal/studio-client.test-runner-helper": "0.2.3-alpha.d5b6464",
    "@types/node": "^22.0.0",
    "ckeditor5": "46.1.1",
    "copyfiles": "^2.4.1",
    "expect": "^30.2.0",
    "global-jsdom": "27.0.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "ckeditor5": "46.1.1"
  },
  "dependencies": {
    "@coremedia/ckeditor5-common": "25.0.4-rc.1",
    "@coremedia/ckeditor5-core-common": "25.0.4-rc.1",
    "@coremedia/ckeditor5-coremedia-richtext": "25.0.4-rc.1",
    "@coremedia/ckeditor5-dom-converter": "25.0.4-rc.1",
    "@coremedia/ckeditor5-dom-support": "25.0.4-rc.1",
    "@coremedia/ckeditor5-logging": "25.0.4-rc.1"
  },
  "scripts": {
    "clean": "pnpm clean:src && pnpm clean:dist",
    "clean:src": "rimraf --glob \"src/**/*.@(js|js.map|d.ts|d.ts.map)\"",
    "clean:dist": "rimraf ./dist",
    "build": "tsc --project ./tsconfig.release.json",
    "postbuild": "node prepare-package.cjs && copyfiles ./README.md dist",
    "npm-check-updates": "npm-check-updates --upgrade",
    "test": "node --import tsx --test __tests__/**/*.test.ts"
  }
}
