{
  "name": "@coremedia/ckeditor5-coremedia-studio-integration-mock",
  "version": "14.0.1-rc.0",
  "description": "CoreMedia Studio: Mock Communication facade for testing",
  "keywords": [
    "coremedia",
    "ckeditor",
    "ckeditor5",
    "studio",
    "mock",
    "testing"
  ],
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc --build src && babel ./src --out-dir ./dist --extensions \".ts,.js\"",
    "postbuild": "copyfiles -u 2 src/theme/* src/theme/**/* dist/theme",
    "clean": "rimraf ./dist",
    "jest": "jest --passWithNoTests",
    "jest:coverage": "jest --collect-coverage --passWithNoTests",
    "npm-check-updates": "npm-check-updates --upgrade"
  },
  "exports": {
    "./*": "./dist/*.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@ckeditor/ckeditor5-core": "^36.0.1",
    "@coremedia-internal/ckeditor5-babel-config": "^1.0.0",
    "@coremedia-internal/ckeditor5-jest-test-helpers": "^1.0.0",
    "@coremedia/service-agent": "^1.1.5",
    "@types/ckeditor__ckeditor5-core": "^33.0.3",
    "@types/ckeditor__ckeditor5-utils": "^28.0.14",
    "@types/jest": "^28.1.8",
    "copyfiles": "^2.4.1",
    "jest": "^28.1.3",
    "jest-each": "^28.1.3",
    "jest-xml-matcher": "^1.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "@ckeditor/ckeditor5-core": "^36.0.1",
    "@coremedia/service-agent": "^1.1.5"
  },
  "dependencies": {
    "@coremedia/ckeditor5-core-common": "14.0.1-rc.0",
    "@coremedia/ckeditor5-coremedia-studio-integration": "14.0.1-rc.0",
    "@coremedia/ckeditor5-logging": "14.0.1-rc.0",
    "rxjs": "^7.5.6"
  }
}
