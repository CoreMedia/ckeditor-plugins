    "@types/jest": "^28.1.8",
    "jest": "^28.1.3",
    "jest-config": "^28.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@coremedia/ckeditor5-logging": "14.0.3-rc.0"
