{
  "name": "@coremedia/ckeditor5-common",
  "version": "15.0.1-rc.1",
  "description": "Common Utilities, independent from CKEditor",
  "keywords": [
    "coremedia"
  ],
  "engines": {
    "node": "18",
    "pnpm": "^8.1"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@coremedia-internal/ckeditor5-jest-test-helpers": "^1.0.0",
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "rimraf": "^5.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@coremedia/ckeditor5-logging": "15.0.1-rc.1"
  },
  "scripts": {
    "clean": "pnpm clean:src && pnpm clean:dist",
    "clean:src": "rimraf --glob \"src/**/*.@(js|js.map|d.ts|d.ts.map)\"",
    "clean:dist": "rimraf ./dist",
    "build": "tsc --project ./tsconfig.release.json",
    "jest": "jest --passWithNoTests",
    "jest:coverage": "jest --collect-coverage --passWithNoTests",
    "npm-check-updates": "npm-check-updates --upgrade"
  }
}
