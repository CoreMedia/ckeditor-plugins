{
  "name": "@coremedia/ckeditor5-common",
  "version": "6.0.1-rc.1",
  "description": "tooling to help with CKEditor 5 Plugin implementations.",
  "keywords": [
    "coremedia"
  ],
  "engines": {
    "node": ">=14.19.0",
    "npm": ">=5.7.1"
  },
  "exports": {
    "./*": "./dist/*.js"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@ckeditor/ckeditor5-core": "^34.0.0",
    "@coremedia-internal/ckeditor5-babel-config": "^1.0.0",
    "@coremedia-internal/ckeditor5-jest-test-helpers": "^1.0.0",
    "@coremedia/types-ckeditor__ckeditor5-core": "6.0.1-rc.1",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^28.1.0",
    "jest-config": "^28.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@ckeditor/ckeditor5-core": "^34.0.0"
  },
  "dependencies": {
    "@coremedia/ckeditor5-logging": "6.0.1-rc.1"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc --build src && babel ./src --out-dir ./dist --extensions \".ts,.js\"",
    "jest": "jest --passWithNoTests",
    "jest:coverage": "jest --collect-coverage --passWithNoTests",
    "lint": "eslint --fix \"src/**\"",
    "prettier": "prettier --write \"src/*.+(js|jsx|ts|tsx|json)\"",
    "npm-check-updates": "npm-check-updates --upgrade"
  }
}
