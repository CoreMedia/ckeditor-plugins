{
  "name": "@coremedia/ckeditor5",
  "version": "1.0.0",
  "description": "Plugins required or recommended to use CKEditor 5 within CoreMedia Studio.",
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "wysiwyg",
    "rich text",
    "editor",
    "html",
    "contentEditable",
    "editing",
    "operational transformation",
    "coremedia",
    "coremedia cms",
    "coremedia studio"
  ],
  "main": "index.js",
  "repository": {
    "url": "https://github.com/CoreMedia/ckeditor-plugins",
    "type": "git"
  },
  "license": "MIT",
  "private": true,
  "workspaces": [
    "types/*",
    "packages/coremedia-utils",
    "packages/coremedia-studio-integration",
    "packages/coremedia-studio-integration-mock",
    "packages/ckeditor5-dataprocessor-support",
    "packages/ckeditor5-symbol-on-paste-mapper",
    "packages/ckeditor5-coremedia-richtext",
    "packages/ckeditor5-coremedia-studio-essentials",
    "packages/ckeditor5-link",
    "packages/jest-test-helpers",
    "app"
  ],
  "scripts": {
    "bootstrap": "pnpm install",
    "publish": "pnpm -r publish",
    "clean": "pnpm -r clean && rimraf ./dist",
    "build": "pnpm -r build",
    "jest": "pnpm -r jest",
    "jest:coverage": "pnpm -r jest:coverage",
    "test:integration": "node --max_old_space_size=8192 node_modules/@ckeditor/ckeditor5-dev-tests/bin/test.js",
    "test:manual": "node --max_old_space_size=8192 node_modules/@ckeditor/ckeditor5-dev-tests/bin/test-manual.js",
    "lint": "pnpm -r lint",
    "prettier": "pnpm -r prettier",
    "npm-check-updates": "pnpm -r npm-check-updates"
  },
  "devDependencies": {
    "pnpm": "^6.9.1",
    "rimraf": "^3.0.2"
  }
}
