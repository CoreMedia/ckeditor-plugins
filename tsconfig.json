{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "dom.iterable",
      "es5",
      "esnext"
    ],
    "module": "commonJS",
    "moduleResolution": "Node",
    "newLine": "LF",
    "removeComments": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES6",
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "types": [
      "jest"
    ]
  }
}
