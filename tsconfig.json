{
  "compilerOptions": {
    "alwaysStrict": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES5",
      "ESNext"
    ],
    "module": "ES6",
    "moduleResolution": "Node",
    "newLine": "LF",
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "ES6",
    "types": [
      "jest"
    ]
  }
}
